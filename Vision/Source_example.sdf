        <visual name="visual">
          <geometry>
            <box>
              <size>0.1 0.1 0.1</size>
            </box>
          </geometry>
        </visual>
        <sensor name="camera" type="camera">
    <camera>
         <save enabled="true">
              <path>/tmp/vision/Cube/cube1</path>
            </save>
            <horizontal_fov>1.047</horizontal_fov>
            <image>
              <width>320</width>
              <height>240</height>
            </image>
            <clip>
              <near>0.1</near>
              <far>100</far>
            </clip>
          </camera>
          <always_on>1</always_on>
          <update_rate>90</update_rate>
          <visualize>true</visualize>
          <topic>camera1</topic>
        </sensor>
